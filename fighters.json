  "playstyle_definitions": {
    
    "aggressive": "Focuses on applying early pressure and maintaining offense",
    "control": "Manages board state and dictates the pace of the game",
    "midrange": "Balanced approach with flexible early-to-mid game plans",
    "combo": "Relies on specific card combinations for powerful effects",
    
    
    "burst": "Delivers high damage in short windows",
    "high_damage": "Consistently deals above-average damage", 
    "consistent": "Reliable, steady damage output", # renamed from consistent_damage
    "finisher": "Specializes in closing out games",
    
    # Defensive Styles
    "tank": "High durability and defensive capabilities", # merged tanky + resilient  
    "elusive": "Avoids damage through positioning and timing",
    "regeneration": "Recovers health or resources over time", # renamed from regenerator
    
    # Resource Management
    "scaling": "Grows stronger over time", # merged power_ramp + late_game concepts
    "late_game": "Peaks in power during extended games", 
    "card_advantage": "Generates extra cards or resources",
    "attrition": "Wins through resource depletion",
    
    # Mechanical Complexity
    "setup": "Requires preparation before major plays", # renamed from set_up
    "tricky": "Complex timing and decision-making",
    "straightforward": "Simple, direct gameplay",
    "unorthodox": "Unique mechanics that break normal rules",
    
    # Positional & Tactical
    "positional": "Success depends heavily on board positioning",
    "swarm": "Uses multiple small units or tokens",
    "harass": "Applies constant small pressure",
    
    # Special Mechanics
    "shapeshifter": "Changes form or abilities during play",
    "recursion": "Reuses cards from discard pile",
    "disruption": "Interferes with opponent's plans", # merged hand_disruption
    "self_harm": "Takes damage to gain advantages", # renamed from self_damage
    "versatile": "Adapts to multiple strategies",
    "glass_cannon": "High damage output but low survivability",
    "information": "Gains knowledge about opponent's hand/deck" # replaced overly specific "deduction"
}